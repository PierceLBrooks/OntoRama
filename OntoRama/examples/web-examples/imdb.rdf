<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE rdf:RDF [
         <!ENTITY rdf 'http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
         <!ENTITY rdfs 'http://www.w3.org/2000/01/rdf-schema#'>
]>
<rdf:RDF xmlns:rdf="&rdf;" xmlns:rdfs="&rdfs;">
<rdfs:Class rdf:ID="Acting"
         rdfs:comment="Actor related information">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdf:Property rdf:ID="Actor"
         rdfs:comment="Actor details">
        <rdfs:domain rdf:resource="#Acting"/>
        <rdfs:range rdf:resource="#ActorInfo"/>
</rdf:Property>
<rdfs:Class rdf:ID="ActorInfo"
         rdfs:comment="Details of actor information">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="AddressOfPlace">
        <rdfs:comment>Address of a place described by building number, street name, city, state, zip, country</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&rdfs;Resource"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Advertising">
        <rdfs:comment>The process of getting the public interested in the movie before and after it is released</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Art">
        <rdfs:comment>People related to creation, design and contruction of Art for the movie  </rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="AwardsandNominations">
        <rdfs:comment>Component describing nominations and awards for various categories</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#IMdb"/>
</rdfs:Class>
<rdfs:Class rdf:ID="AwardsCollection">
        <rdfs:comment>Collection of awards; subclass of Bag</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&rdf;Bag"/>
</rdfs:Class>
<rdf:Property rdf:ID="BuildingNumber"
         rdfs:comment="Building number">
        <rdfs:domain rdf:resource="#AddressOfPlace"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdfs:Class rdf:ID="Business"
         rdfs:comment="Budget and receipts information">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Casting">
        <rdfs:comment>Information on people and agencies involved in selecting actors for the movie</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdf:Property rdf:ID="Certification"
         rdfs:comment="Certification info, country-wise">
        <rdfs:range rdf:resource="#CountryRating"/>
        <rdfs:domain rdf:resource="#MainMovieInfo"/>
</rdf:Property>
<rdfs:Class rdf:ID="CertificationCollection">
         <rdfs:comment>List of Certification(s)</rdfs:comment>
         <rdfs:subClassOf rdf:resource="&rdf;Bag"/>
</rdfs:Class>
<rdf:Property rdf:ID="CertificationList"
         rdfs:comment="points to Certification list">
        <rdfs:range rdf:resource="#CertificationCollection"/>
        <rdfs:domain rdf:resource="#MainMovieInfo"/>
</rdf:Property>
<rdfs:Class rdf:ID="Cinematographer"
         rdfs:comment="describes more specifically the responsibilities of this job">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Cinematography">
        <rdfs:comment>Also called Photography. Information on people and techniques related to it.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdf:Property rdf:ID="CityName"
         rdfs:comment="City name">
        <rdfs:domain rdf:resource="#AddressOfPlace"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="Color">
        <rdfs:comment>[CDATA[Color/B and W]]</rdfs:comment>
        <rdfs:domain rdf:resource="#MainMovieInfo"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdfs:Class rdf:ID="ComposedSongs"
         rdfs:comment="Song details">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Compostion"
         rdfs:comment="Information on songs (composed for)/(used in) the movie">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="CostumeDesign"
         rdfs:comment="details of people and things related to designing costumes">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdf:Property rdf:ID="Country"
         rdfs:comment="Country where movie is screened">
        <rdfs:domain rdf:resource="#CountryRating"/>
        <rdfs:domain rdf:resource="#LengthInfo"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="CountryName"
         rdfs:comment="Country Name">
        <rdfs:domain rdf:resource="#AddressOfPlace"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdfs:Class rdf:ID="CountryRating">
        <rdfs:comment>Certification prescribed by censors of each country where movie is screened</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Date"
         rdfs:comment="Definition of Date day, month, year, dayofweek">
        <rdfs:subClassOf rdf:resource="&rdfs;Resource"/>
</rdfs:Class>
<rdf:Property rdf:ID="DateOfRelease"
         rdfs:comment="Date of release in origin country">
        <rdfs:range rdf:resource="#Date"/>
        <rdfs:domain rdf:resource="#MainMovieInfo"/>
</rdf:Property>
<rdf:Property rdf:ID="Day">
        <rdfs:domain rdf:resource="#Date"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="DayOfWeek">
        <rdfs:domain rdf:resource="#Date"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdfs:Class rdf:ID="Direction">
        <rdfs:comment>Information on people who direct the movie - chief and assistants</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdf:Property rdf:ID="DirectorsCut">
        <rdfs:comment>Length of the movie as desired by the director
and shown to select audiences</rdfs:comment>
        <rdfs:domain rdf:resource="#LengthInfo"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdfs:Class rdf:ID="Distribution">
        <rdfs:comment>Information on distribution company, format of distribution etc.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Distributor"
         rdfs:comment="Distributor specifics">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="DistributorCollection">
        <rdfs:comment>List of distributors</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&rdf;Bag"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Editing"
         rdfs:comment="Information on people and places where editing was done">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdf:Property rdf:ID="FinalLength"
         rdfs:comment="Length of the movie as shown in theatres">
        <rdfs:domain rdf:resource="#LengthInfo"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="Genre"
         rdfs:comment="Type of movie">
        <rdfs:range rdf:resource="#GenreInstance"/>
        <rdfs:domain rdf:resource="#MainMovieInfo"/>
</rdf:Property>
<rdfs:Class rdf:ID="GenreInstance"
         rdfs:comment="Describes specific genre and keywords associated with it">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="GenreCollection">
         <rdfs:comment>List of GenreInstance(s)</rdfs:comment>
         <rdfs:subClassOf rdf:resource="&rdf;Bag"/>
</rdfs:Class>
<rdf:Property rdf:ID="GenreList"
         rdfs:comment="points to list of GenreInstances">
        <rdfs:range rdf:resource="#GenreCollection"/>
        <rdfs:domain rdf:resource="#MainMovieInfo"/>
</rdf:Property>
<rdf:Property rdf:ID="Hour">
        <rdfs:domain rdf:resource="#Time"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdfs:Class rdf:ID="IMdb"
         rdfs:comment="IMdb Service">
        <rdfs:subClassOf rdf:resource="&rdfs;Resource"/>
</rdfs:Class>
<rdf:Property rdf:ID="Language"
         rdfs:comment="Principal language used in the movie">
        <rdfs:domain rdf:resource="#MainMovieInfo"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="Length"
         rdfs:comment="Information regarding the length of the movie">
        <rdfs:range rdf:resource="#LengthInfo"/>
        <rdfs:domain rdf:resource="#MainMovieInfo"/>
</rdf:Property>
<rdfs:Class rdf:ID="LengthInfo"
         rdfs:comment="Describes movie length info (final, directors cut, units)">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="LengthCollection">
         <rdfs:comment>List of LengthInfo</rdfs:comment>
         <rdfs:subClassOf rdf:resource="&rdf;Bag"/>
</rdfs:Class>
<rdf:Property rdf:ID="LengthList"
         rdfs:comment="points to list of LengthInfo">
        <rdfs:range rdf:resource="#LengthCollection"/>
        <rdfs:domain rdf:resource="#MainMovieInfo"/>
</rdf:Property>
<rdfs:Class rdf:ID="LocationDetail"
         rdfs:comment="detailed info of theatres screening movies all over the US">
        <rdfs:subClassOf rdf:resource="#SchedulesandLocation"/>
</rdfs:Class>
<rdfs:Class rdf:ID="LocationInfo"
         rdfs:comment="contains a slot pointing to LocationDetail subclass">
        <rdfs:subClassOf rdf:resource="#SchedulesandLocation"/>
</rdfs:Class>
<rdfs:Class rdf:ID="MainMovieInfo"
         rdfs:comment="Main details of movie like Name, language etc.">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Makeup"
         rdfs:comment="Information on people involved in make up">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdf:Property rdf:ID="Minute">
        <rdfs:domain rdf:resource="#Time"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="Month">
        <rdfs:domain rdf:resource="#Date"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdfs:Class rdf:ID="Movie">
        <rdfs:comment>Central movie related information - processes and people are described by this class</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#IMdb"/>
</rdfs:Class>
<rdfs:Class rdf:ID="MovieCompany"
         rdfs:comment="Template to describe companies engaged in the Movie business">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="MoviePerson"
         rdfs:comment="Person who belongs to movies">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="MovieReleaseInfo"
         rdfs:comment="details of format and year of movie release">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Music"
         rdfs:comment="Music score details">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdf:Property rdf:ID="OriginCountry"
         rdfs:comment="Country where the movie was made or first released">
        <rdfs:domain rdf:resource="#MainMovieInfo"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdfs:Class rdf:ID="Production">
        <rdfs:comment>Information on producers, production companies and other production processes</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="productionDesignCollection">
        <rdfs:comment>List of production designers</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&rdf;Bag"/>
</rdfs:Class>
<rdfs:Class rdf:ID="productionChiefCollection">
        <rdfs:comment>List of chief producers</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&rdf;Bag"/>
</rdfs:Class>
<rdfs:Class rdf:ID="productionManagerCollection">
        <rdfs:comment>List of production managers</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&rdf;Bag"/>
</rdfs:Class>
<rdfs:Class rdf:ID="coproducersCollection">
        <rdfs:comment>List of associate/co-producers</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&rdf;Bag"/>
</rdfs:Class>
<rdfs:Class rdf:ID="executiveProducersCollection">
        <rdfs:comment>List of executive producers</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&rdf;Bag"/>
</rdfs:Class>
<rdfs:Class rdf:ID="productionCompanyCollection">
        <rdfs:comment>List of production companies</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&rdf;Bag"/>
</rdfs:Class>
<rdf:Property rdf:ID="Rating">
        <rdfs:comment>Country specific rating
e.g PG-13(US), 12(UK) M(Australia).</rdfs:comment>
        <rdfs:domain rdf:resource="#CountryRating"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdfs:Class rdf:ID="Review"
         rdfs:comment="Review process information">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="ReviewPerson"
         rdfs:comment="Reviewer details">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="SchedulesandLocation"
         rdfs:comment="Component for describing show timings and locations">
        <rdfs:subClassOf rdf:resource="#IMdb"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Scripting">
        <rdfs:comment>Information on people and events related to writing of the story of the movie</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="scriptWriterCollection">
        <rdfs:comment>List of script writers</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&rdf;Bag"/>
</rdfs:Class>
<rdf:Property rdf:ID="Second">
        <rdfs:domain rdf:resource="#Time"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdfs:Class rdf:ID="ShowDayInfo"
         rdfs:comment="Per day show info for upto 1 week ahead">
        <rdfs:subClassOf rdf:resource="#SchedulesandLocation"/>
</rdfs:Class>
<rdfs:Class rdf:ID="ShowInfo"
         rdfs:comment="contains details of showtimes for each movie">
        <rdfs:subClassOf rdf:resource="#SchedulesandLocation"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Sound">
        <rdfs:comment>Information on the Sound creation, mixing etc processes and people who do them</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="SpecialEffects">
        <rdfs:comment>Information on people and techniques used to generate special effects, if any</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdf:Property rdf:ID="StateName"
         rdfs:comment="State name">
        <rdfs:domain rdf:resource="#AddressOfPlace"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="StreetName1"
         rdfs:comment="Street name">
        <rdfs:domain rdf:resource="#AddressOfPlace"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="StreetName2"
         rdfs:comment="More street name info">
        <rdfs:domain rdf:resource="#AddressOfPlace"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdfs:Class rdf:ID="StuntDoublePerson"
         rdfs:comment="Info on person who performs stunts instead of an actor">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Stunts"
         rdfs:comment="Information on stunts performed in the movie">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdfs:Class rdf:ID="Time"
         rdfs:comment="Definition of Time HH:MM:SS">
        <rdfs:subClassOf rdf:resource="&rdfs;Resource"/>
</rdfs:Class>
<rdf:Property rdf:ID="Title"
         rdfs:comment="Name of the movie">
        <rdfs:domain rdf:resource="#MainMovieInfo"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="Units">
        <rdfs:comment>Units used to measure the length of the film - usually minutes.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdfs:Class rdf:ID="VisualEffects"
         rdfs:comment="Info on people who create visual effects for the movie">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdf:Property rdf:ID="Year">
        <rdfs:domain rdf:resource="#Date"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="ZIP"
         rdfs:comment="ZIP info">
        <rdfs:domain rdf:resource="#AddressOfPlace"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="actorPerson"
         rdfs:comment="points to MoviePerson">
        <rdfs:domain rdf:resource="#ActorInfo"/>
        <rdfs:range rdf:resource="#MoviePerson"/>
</rdf:Property>
<rdf:Property rdf:ID="actorRating"
         rdfs:comment="Rating of actor for his/her part in the movie">
        <rdfs:domain rdf:resource="#ActorInfo"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="actorsAgent"
         rdfs:comment="Actor&apos;s agent details">
        <rdfs:domain rdf:resource="#ActorInfo"/>
        <rdfs:range rdf:resource="#MovieCompany"/>
</rdf:Property>
<rdfs:Class rdf:ID="advertAgency"
         rdfs:comment="info on Agency that advertises the movie">
        <rdfs:subClassOf rdf:resource="#Movie"/>
</rdfs:Class>
<rdf:Property rdf:ID="advertCompany"
         rdfs:comment="Details of the company that does the adertising">
        <rdfs:range rdf:resource="#MovieCompany"/>
        <rdfs:domain rdf:resource="#advertAgency"/>
</rdf:Property>
<rdf:Property rdf:ID="advertisesOnMedium">
        <rdfs:comment>specific medium that this advertiser uses to advertise the movie</rdfs:comment>
        <rdfs:domain rdf:resource="#advertAgency"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="advertisingCost"
         rdfs:comment="Total cost of advertising">
        <rdfs:domain rdf:resource="#Advertising"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="alsoDoes">
        <rdfs:comment>[CDATA[describes other movie related processes that this person can engage in (e.g. lead actor and director)]]</rdfs:comment>
        <rdfs:domain rdf:resource="#MoviePerson"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="artDirectedBy"
         rdfs:comment="info on Art director">
        <rdfs:domain rdf:resource="#Art"/>
        <rdfs:range rdf:resource="#MoviePerson"/>
</rdf:Property>
<rdf:Property rdf:ID="artProcess"
         rdfs:comment="points to Art">
        <rdfs:range rdf:resource="#Art"/>
        <rdfs:domain rdf:resource="#preProductionProcess"/>
</rdf:Property>
<rdf:Property rdf:ID="asstCinematographer"
         rdfs:comment="Assistant to the Chief">
        <rdfs:domain rdf:resource="#Cinematographer"/>
        <rdfs:range rdf:resource="#MoviePerson"/>
</rdf:Property>
<rdf:Property rdf:ID="asstMusicDirector"
         rdfs:comment="details of assistant music director(s)">
        <rdfs:range rdf:resource="#MoviePerson"/>
        <rdfs:domain rdf:resource="#Music"/>
</rdf:Property>
<rdf:Property rdf:ID="asstSoundDesigner"
         rdfs:comment="Details of assistant sound designers">
        <rdfs:range rdf:resource="#MoviePerson"/>
        <rdfs:domain rdf:resource="#Sound"/>
</rdf:Property>
<rdf:Property rdf:ID="asstSoundEditor"
         rdfs:comment="Details of editing assistant(s)">
        <rdfs:range rdf:resource="#MoviePerson"/>
        <rdfs:domain rdf:resource="#Sound"/>
</rdf:Property>
<rdf:Property rdf:ID="asstToDirector"
         rdfs:comment="details of assistant directors">
        <rdfs:domain rdf:resource="#Direction"/>
        <rdfs:range rdf:resource="#MoviePerson"/>
</rdf:Property>
<rdf:Property rdf:ID="awardCategory"
         rdfs:comment="Category in which the award is given">
        <rdfs:domain rdf:resource="#AwardsandNominations"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="awardName"
         rdfs:comment="Name of the award, e.g. Oscar">
        <rdfs:domain rdf:resource="#AwardsandNominations"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="awardOrganization"
         rdfs:comment="Organization that gives out the award">
        <rdfs:domain rdf:resource="#AwardsandNominations"/>
        <rdfs:range rdf:resource="#MovieCompany"/>
</rdf:Property>
<rdf:Property rdf:ID="awardResult"
         rdfs:comment="won/nominated">
        <rdfs:domain rdf:resource="#AwardsandNominations"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="awardYear"
         rdfs:comment="Year for which the movie was nominated/awarded">
        <rdfs:domain rdf:resource="#AwardsandNominations"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="awardsWon"
         rdfs:comment="points to information regarding awards won">
        <rdfs:range rdf:resource="#AwardsandNominations"/>
        <rdfs:domain rdf:resource="#MoviePerson"/>
</rdf:Property>
<rdf:Property rdf:ID="awardsList"
         rdfs:comment="points to list of awards">
         <rdfs:range rdf:resource="#AwardsCollection"/>
         <rdfs:domain rdf:resource="#MoviePerson"/>
</rdf:Property>
<rdf:Property rdf:ID="backgroundMusicType"
         rdfs:comment="general background music in the entire movie">
        <rdfs:domain rdf:resource="#Music"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="backgroundScoreBy"
         rdfs:comment="details of orchestra or band that provided background score">
        <rdfs:range rdf:resource="#MovieCompany"/>
        <rdfs:domain rdf:resource="#Music"/>
</rdf:Property>
<rdf:Property rdf:ID="belongsTo">
        <rdfs:comment>Organization the person belongs to, e.g, the Writers Guild, Actors Guild etc.</rdfs:comment>
        <rdfs:range rdf:resource="#MovieCompany"/>
        <rdfs:domain rdf:resource="#MoviePerson"/>
</rdf:Property>
<rdf:Property rdf:ID="boxOfficeSales"
         rdfs:comment="money earned at box office (in millions of $)">
        <rdfs:domain rdf:resource="#Business"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="castingAgency">
        <rdfs:comment>info on the casting agency that liased with the casting person to select actors</rdfs:comment>
        <rdfs:domain rdf:resource="#Casting"/>
        <rdfs:range rdf:resource="#MovieCompany"/>
</rdf:Property>
<rdf:Property rdf:ID="castingBy"
         rdfs:comment="info on person(s) involved in casting">
        <rdfs:domain rdf:resource="#Casting"/>
        <rdfs:range rdf:resource="#MoviePerson"/>
</rdf:Property>
<rdf:Property rdf:ID="cgAnimator"
         rdfs:comment="details of computer graphics animator(s)">
        <rdfs:range rdf:resource="#MoviePerson"/>
        <rdfs:domain rdf:resource="#VisualEffects"/>
</rdf:Property>
<rdf:Property rdf:ID="cgArtist"
         rdfs:comment="details of computer graphics artist(s)">
        <rdfs:range rdf:resource="#MoviePerson"/>
        <rdfs:domain rdf:resource="#VisualEffects"/>
</rdf:Property>
<rdf:Property rdf:ID="characterName"
         rdfs:comment="Name of character played by actor in this movie">
        <rdfs:domain rdf:resource="#ActorInfo"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
<rdf:Property rdf:ID="chiefCinematographer"
         rdfs:comment="Director of Photography">
        <rdfs:domain rdf:resource="#Cinematographer"/>
        <rdfs:range rdf:resource="#MoviePerson"/>
