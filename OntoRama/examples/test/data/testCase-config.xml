<!--
http://www.gotomy.com/color.html
subtype:	blue:33CCFF
instance:	pink:FF99CC
synonym:	orange:FFCC99
part:		green:00FFCC
member:		yellow:FFFFCC
substance: 	gray:C0C0C0 
inclusive: 	purple:9999FF
-->
<ontConfig>
	<ontology>
		<relation id="1">
			<relationType name="subtype" mappingSymbol="&lt;"/>
			<relationType name="supertype" mappingSymbol="&gt;"/>
			<display color="#33CCFF" symbol="&lt;"/>
		</relation>
		<relation id="2">
			<relationType name="similar" mappingSymbol="~"/>
			<relationType name="equal" mappingSymbol="="/>
			<display color="#33CCFF" symbol="~"/>
		</relation>
		<relation id="3">
			<relationType name="reverse" mappingSymbol="-"/>
			<display color="#33CCFF" symbol="-"/>
		</relation>
		<relation id="4">
			<relationType name="partOf" mappingSymbol="P"/>
			<relationType name="part" mappingSymbol="p"/>
			<display color="#00FFCC" symbol="p"/>
		</relation>
		<relation id="5">
			<relationType name="substanceOf" mappingSymbol="S"/>
			<relationType name="substance" mappingSymbol="s"/>
			<display color="#FF99CC" symbol="s"/>
		</relation>
		<relation id="6">
			<relationType name="instanceOf" mappingSymbol="^"/>
			<relationType name="instance" mappingSymbol=":"/>
			<display color="#FF99CC" symbol="^"/>
		</relation>
		<relation id="7">
			<relationType name="exclusion" mappingSymbol="!"/>
			<relationType name="complement" mappingSymbol="/"/>
			<display color="#FF99CC" symbol="/"/>
		</relation>
		<relation id="8">
			<relationType name="locationOf" mappingSymbol="L"/>
			<relationType name="location" mappingSymbol="l"/>
			<display color="#FF99CC" symbol="l"/>
		</relation>
		<relation id="9">
			<relationType name="memberOf" mappingSymbol="M"/>
			<relationType name="member" mappingSymbol="m"/>
			<display color="#FFFFCC" symbol="m"/>
		</relation>
		<relation id="10">
			<relationType name="objectOf" mappingSymbol="0"/>
			<relationType name="object" mappingSymbol="o"/>
			<display color="#C0C0C0" symbol="0"/>
		</relation>
		<relation id="11">
			<relationType name="urlOf" mappingSymbol="U"/>
			<relationType name="url" mappingSymbol="u"/>
			<display color="#C0C0C0" symbol="u"/>
		</relation>
		<relation id="12">
			<relationType name="nounTypeOf" mappingSymbol="N"/>
			<relationType name="nounType" mappingSymbol="n"/>
			<display color="#C0C0C0" symbol="N"/>
		</relation>
		<conceptProperty id="Description"/>
		<conceptProperty id="Creator"/>
		<conceptProperty id="Synonym"/>
	</ontology>
	<rdfMapping>
		<relationLinks>
			<map id="1" type="supertype" tag="subClassOf"/>
			<map id="2" type="similar" tag="similar"/>
			<map id="4" type="part" tag="part"/>
			<map id="5" type="substance" tag="substance"/>
			<map id="6" type="instanceOf" tag="type"/>
			<map id="7" type="complement" tag="complementOf"/>
			<map id="8" type="location" tag="location"/>
			<map id="9" type="member" tag="wnMember"/>
			<map id="10" type="object" tag="wnObject"/>
			<map id="11" type="url" tag="url"/>
		</relationLinks>
		<conceptProperties>
			<map id="Description" tag="comment"/>
			<map id="Creator" tag="Creator"/>
			<map id="Synonym" tag="label"/>
		</conceptProperties>
	</rdfMapping>
</ontConfig>
