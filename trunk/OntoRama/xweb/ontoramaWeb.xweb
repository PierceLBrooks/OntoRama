			<file sourceFile="img/OntoramaLogo.jpg" targetFile="img/OntoramaLogo.jpg" type="copy"/>
			<file sourceFile="img/ontorama-screenshot.png" targetFile="img/ontorama-screenshot.png" type="copy"/>
