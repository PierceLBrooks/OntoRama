	<application-desc main-class="ontorama.ui.OntoRamaApp"/>
	<resources>
		<j2se version="1.4+"/>
		<jar href="OntoRama.jar"/>
		<jar href="lib/jdom.jar"/>
		<jar href="lib/jenaOntorama.jar"/>
		<jar href="lib/junit.jar"/>
		<jar href="lib/xerces.jar"/>
        <jar href="lib/canvas.jar"/>
        <jar href="lib/events.jar"/>
	</resources>
</jnlp>
