<?xml version="1.0" encoding="utf-8"?>
<!-- JNLP File for OntoRama Application -->
<jnlp spec="1.0+" codebase="http://www.ontorama.org/deployWebstart/" href="OntoRama.jnlp">
	<information>
		<title>OntoRama Demo Application</title>
		<vendor>DSTC, KVO (GU)</vendor>
		<homepage href="http://www.ontorama.org"/>
		<description>Ontology Browser Prototype</description>
		<description kind="tooltip">GO OntoRama!</description>
		<icon href="img/OntoramaLogo-smaller.jpg"/>
		<offline-allowed/>
