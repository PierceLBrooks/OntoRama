	<layout>
		<sectionStyle type="home">
			<image type="normalSectionButton"/>
			<image type="activeSectionButton"/>
			<!--<image type="sectionBanner"/>-->
		</sectionStyle>	
		<sectionStyle type="img">
		</sectionStyle>
		<sectionStyle type="doc">
			<image type="normalSectionButton"/>
			<image type="activeSectionButton"/>
			<!--<image type="sectionBanner"/>-->
		</sectionStyle>	
		<sectionStyle type="demo">
			<image type="normalSectionButton"/>
			<image type="activeSectionButton"/>
			<!--<image type="sectionBanner"/>-->
		</sectionStyle>	
		<sectionStyle type="plans">
